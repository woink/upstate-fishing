# Testing
# Set to 'true' to run integration tests against live APIs
RUN_INTEGRATION_TESTS=false

# Weather.gov User-Agent (optional)
# Weather.gov requires a User-Agent identifying your app. Override the default here.
# WEATHER_USER_AGENT=MyApp/1.0 (contact@example.com)

# Supabase
SUPABASE_URL=http://localhost:54321
SUPABASE_ANON_KEY=your-local-anon-key
# WARNING: Service role key bypasses Row Level Security. Never expose to browser/client code.
SUPABASE_SERVICE_ROLE_KEY=your-local-service-role-key

# Supabase integration tests (requires 'supabase start')
RUN_SUPABASE_TESTS=false
